{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/sheets/cubesheet.jsx", "app/api/projects/[projectId]/inventory/[itemId]/route.js", "app/api/projects/[projectId]/inventory/route.js", "app/api/projects/[projectId]/route.js", "app/api/projects/[projectId]/spreadsheet/route.js", "app/projects/page.jsx", "app/projects/[projectId]/page.jsx", "components/InventoryManager.jsx", "components/sheets/Spreadsheet.jsx", "components/sheets/Spreadsheet.jsx", "components/sheets/Spreadsheet.jsx", "lib/mongodb.js"],
  "exclude": ["node_modules"]
}
